    "@angular/platform-browser-dynamic": "~10.1.6",
    "@angular/router": "~10.1.6",
    "@angular/service-worker": "~10.1.6",
    "@schematics/angular": "^10.2.0",
    "ngx-indexed-db": "^6.1.0",
